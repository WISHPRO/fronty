{
  "name": "fronty",
  "version": "2.3.0",
  "description": "Fronty is a boilerplate for starting new front-end projects.",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git@github.com:salencebg/fronty.git"
  },
  "scripts": {
    "test": "mocha -R spec assets/js/tests/**/*.js",
    "postinstall": "wget -O sass/lib/_normalize.scss https://raw.githubusercontent.com/necolas/normalize.css/master/normalize.css"
  },
  "author": "Aleksandar Go≈°evski <gosevskibg@gmail.com>",
  "license": "BSD",
  "dependencies": {
    "lodash": "^3.3.0"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-consolidate": "^0.1.2",
    "gulp-css-image-dimensions": "^1.1.0",
    "gulp-iconfont": "^1.0.0",
    "gulp-imagemin": "^2.2.0",
    "gulp-jshint": "^1.9.2",
    "gulp-minify-css": "^0.4.6",
    "gulp-plumber": "^0.6.6",
    "gulp-sass": "^1.3.3",
    "gulp-svg-spritesheet": "0.0.3",
    "gulp-svg2png": "^0.3.0",
    "gulp-svgo": "^0.1.1",
    "gulp-uglify": "^1.1.0",
    "jshint-stylish": "^1.0.0",
    "vinyl-transform": "^1.0.0"
  },
  "jshintConfig": {
    "bitwise": false,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": false,
    "plusplus": false,
    "regexp": false,
    "undef": true,
    "strict": false,
    "trailing": true,
    "asi": false,
    "boss": true,
    "debug": true,
    "eqnull": false,
    "esnext": true,
    "evil": false,
    "expr": true,
    "funcscope": false,
    "globalstrict": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": false,
    "laxcomma": true,
    "loopfunc": true,
    "multistr": true,
    "proto": false,
    "regexdash": false,
    "scripturl": false,
    "smarttabs": false,
    "shadow": false,
    "sub": true,
    "supernew": false,
    "validthis": false,
    "browser": true,
    "couch": false,
    "devel": true,
    "dojo": false,
    "jquery": false,
    "mootools": false,
    "node": true,
    "nonstandard": true,
    "predef": [
      "define",
      "require",
      "$",
      "beforeEach",
      "afterEach",
      "jasmine",
      "it",
      "describe",
      "expect",
      "sinon"
    ],
    "prototypejs": false,
    "rhino": false,
    "wsh": false,
    "onevar": false,
    "passfail": false
  }
}
